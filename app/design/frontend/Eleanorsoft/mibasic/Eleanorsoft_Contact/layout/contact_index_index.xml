<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="help" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="help_menu">
            <arguments>
                <argument name="active_item" xsi:type="string">contact</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="contactForm">
            <action method='setTemplate' ifconfig="contact/ajax_contacts/enabled">
                <argument name='template' xsi:type='string'>Eleanorsoft_Contact::form.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>